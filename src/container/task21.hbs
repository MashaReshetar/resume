<div>
    <main>
        <section>
            {{#with heading}}

                {{#with main}}
                    <div class="card">
                        <div class="card-body">
                            <h1>{{title}}</h1>
                            <p>{{text}}</p>
                            <button type="button" class="btn btn-primary">{{button_text.solid}}</button> 
                            <br><br>
                            <button type="button" class="btn btn-outline-primary">{{button_text.outline}}</button>
                        </div>
                    </div>
                {{/with}}

                <br>

                {{#with second}}
                    <div class="card">
                        <div class="card-body">
                            <h2>{{title}}</h3>
                            <p>{{text}}</p>
                            <button type="button" class="btn btn-secondary">{{button_text.solid}}</button> 
                            <br><br>
                            <button type="button" class="btn btn-outline-secondary">{{button_text.outline}}</button>
                        </div>
                    </div>
                {{/with}}

                <br>

                {{#with subblock}}
                    <div class="card">
                        <div class="card-body">
                            <h3>{{title}}</h3>
                            <p>{{text}}</p>
                            <button type="button" class="btn btn-dark">{{button_text.solid}}</button> 
                            <br><br>
                            <button type="button" class="btn btn-outline-dark">{{button_text.outline}}</button>
                        </div>
                    </div>
                {{/with}}
            {{/with}}
                
                <br>

                {{#with nested_display}}
                    <div class="card">
                        <div class="card-body">
                            <h1 class="display-1">{{title}}</h1>

                                {{#with children_display}}
                                    <div class="card">
                                        <div class="card-body">
                                            <h2 class="display-4">{{title}}</h2>
                                            <p>{{text}}</p>

                                            {{#with button}}
                                            <button type="button" class="btn btn-outline-warning">{{text}}</button>
                                            {{/with}}
                                        </div>
                                    </div>
                                {{/with}}
                                <br>

                            <h6>{{header_text}}</h6>
                            {{#with button}}
                                <button type="button" class="btn btn-info">{{text}}</button>
                            {{/with}}
                        </div>
                    </div>
                {{/with}}

                <br>

                {{#with paragraph}}
                    <div class="card">
                        <div class="card-body">
                            <p class="lead">{{title}}</p>
                            <p>{{text}}</p>

                            {{#with button}}
                                <a class="btn btn-link" href="{{url}}" role="button">{{text}}</a>
                            {{/with}}

                            <br>
                            <br>

                            <div class="card">
                                <div class="card-body">

                                    {{#each list_name}}
                                        <ul>
                                            <li>
                                                <a href="{{url}}">{{text}}</a>
                                            </li>
                                        </ul>
                                    {{/each}}
                                </div>
                            </div>
                        </div>
                    </div>

                    
                {{/with}}


                
            

        </section>
    </main>
</div>